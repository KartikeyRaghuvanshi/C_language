//Program 1
#include <stdio.h>

void main()
{
    int num,i;
    
    printf("enter the number:");
    scanf("%d",&num);
    
    for(;num>=0;num--)
    {
        printf("%d ",num);    
    }
}

//output of program 1--> if you input 5. (5 4 3 2 1 0)


