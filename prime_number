// prime number or not

#include <stdio.h>

void main()
{
    int num,i;
    int count=0;
    
    printf("enter a number:");
    scanf("%d",&num);
    
    for(i=2;i<num;i++)
    {
        if(num%i==0)
        {
            count++;
            break;
        }
    }
    if (count==0)
    {
        printf("is a prime number.");
    }
    else
    {
       printf(" is not a prime number"); 
    }
}
